{
    "version": 2,
    "builds": [{
        "src": "pecuaria_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"
        }
    }],
    "routes": [{
        "src": "/static/(.*)",
        "dest": "/static/$1"
    }, {
        "src": "/(.*)",
        "dest": "pecuaria_project/wsgi.py"
    }],
    "env": {
        "DJANGO_SETTINGS_MODULE": "pecuaria_project.settings",
        "PYTHONPATH": ".",
        "TZ": "UTC"
    }
}
